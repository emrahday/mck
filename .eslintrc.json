{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true,
		"node": true,
		"jest": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-empty": "off",
		"indent": ["error", "tab"],
		"no-tabs": 0,
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}